#: pyhello.py:43
#, python-format
msgid "Usage: %s [OPTION]...\n"
msgstr "Modo de empleo: %s [OPCIÓN]\n\n"

#: pyhello.py:44
msgid "Print a friendly, customizable greeting.\n"
msgstr "Imprime un saludo configurable\n"

#: pyhello.py:46
msgid "  -t, --traditional       use traditional greeting\n"
msgstr "  -t, --traditional       usa el saludo tradicional\n"

#: pyhello.py:47
msgid "  -g, --greeting=TEXT     use TEXT as the greeting message\n"
msgstr "  -g, --greeting=TEXT     usa el texto TEXT como mensaje de saludo\n"

#: pyhello.py:49
msgid "      --help     display this help and exit\n"
msgstr "      --help     muestra esta ayuda y sale\n"

#: pyhello.py:50
msgid "      --version  output version information and exit\n"
msgstr "      --version  muestra la información de versión y sale\n"

#: pyhello.py:52
msgid "General help using GNU software: <http://www.gnu.org/gethelp/>\n"
msgstr "Ayuda en general para usar el software GNU: <http://www.gnu.org/gethelp/>\n"

#: pyhello.py:86
msgid "hello, world"
msgstr "hola, mundo"

#: pyhello.py:92
msgid "Hello, world!"
msgstr "¡Hola, mundo!"
